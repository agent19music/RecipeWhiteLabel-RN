# Rename this file to .env and add your API keys

# OpenAI API Key (fallback if Gemini fails)
# Get your key from: https://platform.openai.com/api-keys
EXPO_PUBLIC_OPENAI_API_KEY=

# Google Gemini API Key (preferred for AI features)
# Get your key from: https://makersuite.google.com/app/apikey
EXPO_PUBLIC_GEMINI_API_KEY=

# Optional: Supabase configuration (if using auth)
EXPO_PUBLIC_SUPABASE_URL=
EXPO_PUBLIC_SUPABASE_ANON_KEY=

# Optional: Analytics
EXPO_PUBLIC_ANALYTICS_ID=